<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rjwl.api.mapper.RoleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rjwl.api.entity.Role">
    <result column="id" property="id" />
        <result column="role_name" property="roleName" />
        <result column="state" property="state" />
        <result column="gmt" property="gmt" />
        <result column="upt" property="upt" />
        <result column="name_state" property="nameState" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        role_name, state, gmt, upt, name_state, version
    </sql>

</mapper>
